{
    "name": "vite-rest",
    "version": "0.0.0",
    "private": true,
    "scripts": {
        "start": "node server.js",
        "build": "cd client && yarn build",
        "install": "cd client && yarn",
        "server": "nodemon --watch './**/*.ts' --exec 'ts-node' server/index.ts",
        "client": "cd client && yarn dev",
        "dev": "concurrently --kill-others 'yarn server' 'yarn client'"
    },
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^1.36.4",
        "concurrently": "^8.0.1",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "jsonwebtoken": "^9.0.0",
        "mongoose": "^7.1.0",
        "morgan": "^1.10.0",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "nodemailer": "^6.9.1",
        "ts-utils-julseb": "^2.2.15"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/morgan": "^1.9.4",
        "@types/multer": "^1.4.7",
        "@types/nodemailer": "^6.4.7",
        "nodemon": "^2.0.22",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.4"
    }
}
